void insertionSort(int arraySize, int *  array) {
    int aux,i,j,k,count=0;
    for(i=0;i<arraySize-1;i++){
       array[count];
       for(j=count+1; j<arraySize;j++){
           if(array[count]>array[j]){
               aux=array[count];
               array[count]=array[j];
               array[j]=aux;
           }
       }
       for(k=0;k<arraySize;k++)
           printf("%d ",array[k]);
       printf("\n");
       count++;
   }
}
